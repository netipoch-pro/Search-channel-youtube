{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/public/manifest.json"
    },
    {
      "src": "/service-worker.js",
      "dest": "/public/service-worker.js"
    },
    {
      "src": "/icon-(.*)",
      "dest": "/public/icon-$1"
    },
    {
      "src": "/offline.html",
      "dest": "/public/offline.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "YOUTUBE_API_KEY": "@youtube-api-key"
  }
}